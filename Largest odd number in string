// run a loop from end of array 
// if any number from right is odd then return substring from 0 to i+1;

string maxOdd(string s) {
       
        string ans= "";
        for(int i=s.length()-1;i>=0;i--){
            if((s[i]-'0')%2!=0){
                ans = s.substr(0,i+1);
                return ans;
            }
        }
    }
